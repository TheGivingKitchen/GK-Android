<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/eventsFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.home.events.EventsFragment"
        android:label="EventsFragment"
        tools:layout="@layout/fragment_events"/>
    <fragment
        android:id="@+id/safetynetFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.home.safetynet.SafetynetFragment"
        android:label="SafetynetFragment"
        tools:layout="@layout/fragment_safetynet" />
    <fragment
        android:id="@+id/assistanceFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.home.assistance.AssistanceFragment"
        android:label="AssistanceFragment"
        tools:layout="@layout/fragment_assistance">
        <action
            android:id="@+id/action_assistanceFragment_to_assistanceInquiryFormPrologueFragment"
            app:destination="@id/assistanceInquiryFormPrologueFragment" />
        <action
            android:id="@+id/action_assistanceFragment_to_assistanceReferralFormPrologueFragment"
            app:destination="@id/assistanceReferralFormPrologueFragment" />
    </fragment>
    <fragment
        android:id="@+id/giveFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.home.give.GiveFragment"
        android:label="GiveFragment"
        tools:layout="@layout/fragment_give">
        <action
            android:id="@+id/action_giveFragment_to_volunteerFormPrologueFragment"
            app:destination="@id/volunteerFormPrologueFragment" />
        <action
            android:id="@+id/action_giveFragment_to_safetynetFormPrologueFragment"
            app:destination="@id/safetynetFormPrologueFragment" />
    </fragment>
    <fragment
        android:id="@+id/volunteerFormPrologueFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.volunteerform.VolunteerFormPrologueFragment"
        android:label="VolunteerFormPrologueFragment" />
    <fragment
        android:id="@+id/safetynetFormPrologueFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.safetynetform.SafetynetFormPrologueFragment"
        android:label="SafetynetFormPrologueFragment" />
    <fragment
        android:id="@+id/assistanceInquiryFormPrologueFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.assistanceinquiryform.AssistanceInquiryFormPrologueFragment"
        android:label="AssistanceInquiryFormPrologueFragment" />
    <fragment
        android:id="@+id/assistanceReferralFormPrologueFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.assistancereferralform.AssistanceReferralFormPrologueFragment"
        android:label="AssistanceReferralFormPrologueFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"/>
</navigation>