<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/eventsFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.events.EventsFragment"
        android:label="EventsFragment"
        tools:layout="@layout/fragment_events" />
    <fragment
        android:id="@+id/safetynetFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.safetynet.safetynettab.SafetynetFragment"
        android:label="SafetynetFragment"
        tools:layout="@layout/fragment_safetynet" >
        <action
            android:id="@+id/action_safetynetFragment_to_facebookGroupsFragment"
            app:destination="@id/facebookGroupsFragment" />
    </fragment>
    <fragment
        android:id="@+id/assistanceFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.assistance.AssistanceFragment"
        android:label="AssistanceFragment"
        tools:layout="@layout/fragment_assistance" >
        <action
            android:id="@+id/action_assistanceFragment_to_formsFragment"
            app:destination="@id/formsFragment" />
    </fragment>
    <fragment
        android:id="@+id/giveFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.give.GiveFragment"
        android:label="GiveFragment"
        tools:layout="@layout/fragment_give" >
        <action
            android:id="@+id/action_giveFragment_to_formsFragment"
            app:destination="@id/formsFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/formsFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.prologue.FormPrologueFragment"
        android:label="FormsFragment" >
        <action
            android:id="@+id/action_formsFragment_to_questionsContainerFragment"
            app:destination="@id/questionsContainerFragment" />
    </fragment>
    <fragment
        android:id="@+id/questionsContainerFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.forms.QuestionsContainerFragment"
        android:label="QuestionsContainerFragment" />
    <fragment
        android:id="@+id/facebookGroupsFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.safetynet.FacebookGroupsFragment"
        android:label="FacebookGroupsFragment" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.about.feedback.FeedbackFragment"
        android:label="FeedbackFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.thegivingkitchen.android.thegivingkitchen.ui.homescreen.about.AboutFragment"
        android:label="AboutFragment" >
        <action
            android:id="@+id/action_aboutFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
    </fragment>
</navigation>